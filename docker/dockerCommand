# Add Docker's official GPG key:
    2  sudo apt update
    3  sudo apt install ca-certificates curl
    4  sudo install -m 0755 -d /etc/apt/keyrings
    5  sudo curl -fsSL https://download.docker.com/linux/ubuntu/gpg -o /etc/apt/keyrings/docker.asc
    6  sudo chmod a+r /etc/apt/keyrings/docker.asc
    7  # Add the repository to Apt sources:
    8  sudo tee /etc/apt/sources.list.d/docker.sources <<EOF
    9  Types: deb
   10  URIs: https://download.docker.com/linux/ubuntu
   11  Suites: $(. /etc/os-release && echo "${UBUNTU_CODENAME:-$VERSION_CODENAME}")
   12  Components: stable
   13  Signed-By: /etc/apt/keyrings/docker.asc
   14  EOF
   15  sudo apt update
   16  sudo apt install docker-ce docker-ce-cli containerd.io docker-buildx-plugin docker-compose-plugin
